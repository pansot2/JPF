<?xml version="1.0" encoding="UTF-8"?>
<project basedir=".." name="jpf-core">
    <property file="nbproject/nbjdk.properties"/>
    <property location="${netbeans.user}/build.properties" name="user.properties.file"/>
    <property file="${user.properties.file}"/>
    <import file="jdk.xml"/>
    <target depends="-jdk-init" name="build">
        <ant inheritall="false" target="build"/>
    </target>
    <target depends="-jdk-init" name="clean">
        <ant inheritall="false" target="clean"/>
    </target>
    <target depends="-jdk-init" name="test-project">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="test-project"/>
    </target>
    <target depends="-jdk-init" name="run-selected-main">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="run-selected-main"/>
    </target>
    <target depends="-jdk-init,-jdk-presetdef-nbjpdastart" name="debug-selected-main">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="debug-selected-main"/>
    </target>
    <target depends="-jdk-init" name="run-selected-example">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="run-selected-example"/>
    </target>
    <target depends="-jdk-init,-jdk-presetdef-nbjpdastart" name="debug-selected-example">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="debug-selected-example"/>
    </target>
    <target depends="-jdk-init" name="run-selected-example-jpf">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="run-selected-example-jpf"/>
    </target>
    <target depends="-jdk-init,-jdk-presetdef-nbjpdastart" name="debug-selected-example-jpf">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="debug-selected-example-jpf"/>
    </target>
    <target depends="-jdk-init" name="run-selected-test">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="run-selected-test"/>
    </target>
    <target depends="-jdk-init,-jdk-presetdef-nbjpdastart" name="debug-selected-test">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="debug-selected-test"/>
    </target>
    <target depends="-jdk-init" name="run-selected-test-jpf">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="run-selected-test-jpf"/>
    </target>
    <target depends="-jdk-init,-jdk-presetdef-nbjpdastart" name="debug-selected-test-jpf">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="debug-selected-test-jpf"/>
    </target>
</project>
